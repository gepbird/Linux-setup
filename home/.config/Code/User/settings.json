{
  "telemetry.telemetryLevel": "off",
  "keyboard.dispatch": "keyCode",
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.openEditors.visible": 0,
  "workbench.activityBar.visible": true,
  "workbench.startupEditor": "none",
  "workbench.iconTheme": "material-icon-theme",
  "workbench.editor.untitled.hint": "hidden",
  "window.title": "${dirty}${seperator}${rootName}",
  "editor.minimap.enabled": false,
  "editor.fontSize": 16,
  "editor.lineNumbers": "relative",
  "editor.suggestSelection": "first",
  "editor.fontFamily": "'Noto Mono', 'Droid Sans Mono', 'Courier New', monospace, 'Droid Sans Fallback'",
  "editor.cursorBlinking": "phase",
  "editor.formatOnPaste": true,
  "editor.formatOnType": true,
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.organizeImports": true,
  },
  "editor.useTabStops": true,
  "editor.bracketPairColorization.enabled": true,
  "editor.inlineSuggest.enabled": false,
  "editor.quickSuggestionsDelay": 999999,
  "editor.quickSuggestions": {
    "other": false,
    "comments": false,
    "strings": false
  },
  "editor.detectIndentation": false,
  "editor.tabCompletion": "off",
  "editor.renderWhitespace": "all",
  "editor.insertSpaces": true,
  "editor.tabSize": 2,
  "files.insertFinalNewline": true,
  "debug.onTaskErrors": "showErrors",
  "terminal.integrated.defaultProfile.windows": "PowerShell",
  "terminal.integrated.defaultProfile.linux": "fish",
  "terminal.integrated.tabs.enabled": true,
  "terminal.integrated.profiles.windows": {
    "PowerShell": {
      "source": "PowerShell",
      "icon": "terminal-powershell"
    },
    "Command Prompt": {
      "path": [
        "${env:windir}\\Sysnative\\cmd.exe",
        "${env:windir}\\System32\\cmd.exe"
      ],
      "args": [],
      "icon": "terminal-cmd"
    },
    "Git Bash": {
      "source": "Git Bash"
    },
    "Windows PowerShell": {
      "path": "C:\\Windows\\System32\\WindowsPowerShell\\v1.0\\powershell.exe"
    }
  },
  "git.enableSmartCommit": true,
  "git.autofetch": true,
  "git.autofetchPeriod": 30,
  "git.confirmSync": false,
  "git.ignoreLimitWarning": true,
  "git.statusLimit": 1000,
  "diffEditor.ignoreTrimWhitespace": false,
  "settingsSync.ignoredSettings": [
    "vim.vimrc.path"
  ],
  "remote.SSH.remotePlatform": {
    "thegep.xyz": "linux"
  },
  "liveServer.settings.donotShowInfoMsg": true,
  "files.exclude": {
    "**/__pycache__": true,
    "**/*.fuse_hidden*": true,
    "**/*.db-journal": true,
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true
  },
  "vim.useCtrlKeys": false,
  "vim.vimrc.enable": true,
  "vim.enableNeovim": true,
  "vim.vimrc.path": "/home/gep/.config/Code/User/.vimrc",
  "github.copilot.enable": {
    "*": true,
    "yaml": true,
    "plaintext": true,
    "markdown": true
  },
  "[html]": {
    "editor.suggest.insertMode": "replace",
    "editor.autoClosingBrackets": "always",
    "editor.insertSpaces": false,
  },
  "[javascript]": {},
  "javascript.updateImportsOnFileMove.enabled": "always",
  "javascript.format.insertSpaceAfterKeywordsInControlFlowStatements": false,
  "javascript.format.placeOpenBraceOnNewLineForControlBlocks": true,
  "javascript.format.placeOpenBraceOnNewLineForFunctions": true,
  "javascript.format.semicolons": "insert",
  "javascript.preferences.quoteStyle": "single",
  "debug.javascript.defaultRuntimeExecutable": {
    "pwa-node": "/usr/bin/node"
  },
  "[typescript]": {},
  "typescript.updateImportsOnFileMove.enabled": "always",
  "typescript.format.insertSpaceAfterKeywordsInControlFlowStatements": false,
  "typescript.format.placeOpenBraceOnNewLineForControlBlocks": true,
  "typescript.format.placeOpenBraceOnNewLineForFunctions": true,
  "typescript.preferences.quoteStyle": "single",
  "js/ts.implicitProjectConfig.experimentalDecorators": true,
  "[dart]": {
    "editor.formatOnSave": true,
    "editor.formatOnType": true,
    "editor.rulers": [
      80,
    ],
    "editor.selectionHighlight": false,
    "editor.suggest.snippetsPreventQuickSuggestions": false,
    "editor.suggestSelection": "first",
    "editor.tabCompletion": "onlySnippets",
    "editor.wordBasedSuggestions": false,
  },
  "dart.devToolsReuseWindows": false,
  "dart.debugSdkLibraries": true,
  "dart.flutterSdkPath": "/opt/flutter",
  "dart.debugExternalPackageLibraries": true,
  "[php]": {
    "editor.defaultFormatter": "Sophisticode.php-formatter",
  },
  "intelephense.telemetry.enabled": false,
  "phpformatter.composer": true,
  "intelephense.format.braces": "allman",
  "intelephense.format.enable": false,
  "intelephense.diagnostics.undefinedTypes": false,
  "intelephense.diagnostics.undefinedFunctions": false,
  "intelephense.diagnostics.undefinedConstants": false,
  "intelephense.diagnostics.undefinedClassConstants": false,
  "intelephense.diagnostics.undefinedMethods": false,
  "intelephense.diagnostics.undefinedProperties": false,
  "intelephense.diagnostics.undefinedVariables": false,
  "[python]": {
    "editor.wordBasedSuggestions": true,
  },
  "python.languageServer": "Pylance",
  "python.formatting.provider": "autopep8",
  "python.formatting.autopep8Args": [
    "--indent-size=2",
    "--max-line-length=160"
  ],
  "[csharp]": {
    "editor.defaultFormatter": "ms-dotnettools.csharp",
  },
  "csharp.semanticHighlighting.enabled": true,
  "csharp.suppressHiddenDiagnostics": false,
  "csharpextensions.useThisForCtorAssignments": false,
  "omnisharp.enableRoslynAnalyzers": true,
  "omnisharp.enableAsyncCompletion": true,
  "omnisharp.enableDecompilationSupport": true,
  "omnisharp.enableImportCompletion": true,
  "[cpp]": {
    "editor.wordBasedSuggestions": false,
    "editor.suggest.insertMode": "replace",
    "editor.semanticHighlighting.enabled": true
  },
  "C_Cpp.formatting": "vcFormat",
  "C_Cpp.vcFormat.space.afterKeywordsInControlFlowStatements": false,
  "C_Cpp.vcFormat.indent.caseLabels": true,
  "C_Cpp.vcFormat.indent.preprocessor": "none",
  "C_Cpp.vcFormat.indent.accessSpecifiers": true,
}
